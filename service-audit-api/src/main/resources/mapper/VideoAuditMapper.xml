  </update>
  <select id="selectByPrimaryKey" parameterType="java.lang.String" resultMap="BaseResultMap">
    select uuid, status, number
    from`cn.scbc.mycinema`.video_audit
    where uuid = #{uuid,jdbcType=VARCHAR}
